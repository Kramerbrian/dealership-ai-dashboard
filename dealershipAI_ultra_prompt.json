{
  "project": "DealershipAI",
  "version": "v1.0.0-final",
  "goals": [
    "Finalize multi-tenant Next.js dashboard with Cupertino design",
    "Complete scoring engine (ATI, AIV, VLI, OI, GBP, RRS, WX, IFR, CIS)",
    "Deploy automated CI/CD with verified webhooks",
    "Generate documentation pack and release summary"
  ],
  "commands": {
    "setup": [
      "./scripts/setup-env.sh",
      "./scripts/sync-vercel-env.sh --force",
      "vercel env ls"
    ],
    "deploy": {
      "preview": "vercel --pre",
      "production": "vercel --prod"
    },
    "verify": [
      "vercel env ls",
      "vercel env pull .env.local",
      "npm run build",
      "npm run lint"
    ]
  },
  "checks": [
    "Environment parity (dev/preview/prod)",
    "Webhook verification (Stripe, Clerk)",
    "Dashboard rendering in both modes",
    "ElasticNet recalibration logs",
    "Score export per tenant"
  ],
  "docs": [
    "README.md",
    "ONBOARDING.md",
    "SCORING.md",
    "AUTOMATIONS.md",
    "API.md"
  ],
  "tagline": "Where signal replaces noise.",
  "principles": {
    "clarity": "How well you're seen",
    "trust": "How much the system believes you"
  },
  "enhancements_deployed": {
    "phase1": {
      "name": "Quick Wins",
      "components": ["ExitIntentModal", "TrustBadges", "UrgencyBanner"],
      "impact": "+30% conversion"
    },
    "phase2": {
      "name": "Revenue Optimization",
      "components": ["MobileMenu", "PricingToggle"],
      "impact": "+20% revenue"
    },
    "phase3": {
      "name": "User Experience",
      "components": ["ErrorBoundary", "EmptyStates"],
      "impact": "-50% support tickets"
    }
  },
  "webhooks": {
    "stripe": "/api/stripe/webhook",
    "clerk": "/api/clerk/webhook"
  },
  "analytics": {
    "events": [
      "exit_intent_conversion",
      "urgency_banner_click",
      "pricing_toggle_changed"
    ]
  },
  "metrics": {
    "expected_arr_increase": 240000,
    "expected_conversion_lift": 0.5,
    "expected_support_reduction": 0.5
  }
}
