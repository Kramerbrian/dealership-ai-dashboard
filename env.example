# DealershipAI Environment Variables
# Copy this file to .env.local and fill in your actual values

# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/dealershipai"
SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_ANON_KEY="your-supabase-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-supabase-service-role-key"

# Redis Configuration
REDIS_URL="redis://localhost:6379"
UPSTASH_REDIS_REST_URL="https://your-redis.upstash.io"
UPSTASH_REDIS_REST_TOKEN="your-upstash-token"

# Authentication
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret"
JWT_SECRET="your-jwt-secret"

# OpenAI Configuration
OPENAI_API_KEY="sk-your-openai-api-key"
OPENAI_ORGANIZATION="your-openai-org-id"

# Email Configuration
RESEND_API_KEY="re_your-resend-api-key"
FROM_EMAIL="noreply@dealershipai.com"

# Analytics
GOOGLE_ANALYTICS_ID="G-XXXXXXXXXX"
GOOGLE_TAG_MANAGER_ID="GTM-XXXXXXX"

# External APIs
GOOGLE_MAPS_API_KEY="your-google-maps-api-key"
GOOGLE_SEARCH_CONSOLE_API_KEY="your-gsc-api-key"

# Feature Flags
ENABLE_ANALYTICS="true"
ENABLE_AI_OPTIMIZATION="true"
ENABLE_TRUST_SCORING="true"
ENABLE_FINE_TUNING="true"

# Rate Limiting
RATE_LIMIT_ENABLED="true"
RATE_LIMIT_REQUESTS_PER_MINUTE="100"
RATE_LIMIT_REQUESTS_PER_HOUR="1000"

# Security
HMAC_SECRET="your-hmac-secret"
WEBHOOK_SECRET="your-webhook-secret"

# Monitoring
SENTRY_DSN="your-sentry-dsn"
LOG_LEVEL="info"

# Development
NODE_ENV="development"
DEBUG="dealershipai:*"

# Production URLs
NEXT_PUBLIC_APP_URL="https://dealershipai.com"
NEXT_PUBLIC_DASHBOARD_URL="https://dash.dealershipai.com"
NEXT_PUBLIC_MARKETING_URL="https://marketing.dealershipai.com"