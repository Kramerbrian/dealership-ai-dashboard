{
  "domain": "dash.dealershipai.com",
  "api_endpoints": {
    "base_url": "https://dash.dealershipai.com",
    "health": "/api/health",
    "quick_audit": "/api/quick-audit",
    "auth": {
      "signin": "/api/auth/signin",
      "signup": "/api/auth/signup",
      "callback": "/api/auth/callback",
      "logout": "/api/auth/logout"
    },
    "dashboard": {
      "overview": "/api/dashboard/overview",
      "metrics": "/api/dashboard/metrics",
      "analytics": "/api/dashboard/analytics"
    },
    "ai_services": {
      "visibility_index": "/api/ai/visibility-index",
      "competitive_analysis": "/api/ai/competitive-analysis",
      "market_trends": "/api/ai/market-trends"
    },
    "dealership": {
      "profile": "/api/dealership/profile",
      "inventory": "/api/dealership/inventory",
      "performance": "/api/dealership/performance"
    }
  },
  "backend_services": {
    "database": {
      "supabase_url": "https://your-project.supabase.co",
      "connection_pool": 20,
      "timeout": 30000
    },
    "authentication": {
      "ory_kratos": "https://optimistic-haslett-3r8udelhc2.projects.oryapis.com",
      "nextauth": {
        "secret": "your-nextauth-secret",
        "url": "https://dash.dealershipai.com"
      }
    },
    "ai_providers": {
      "openai": {
        "api_key": "your-openai-key",
        "model": "gpt-4",
        "max_tokens": 4000
      },
      "anthropic": {
        "api_key": "your-anthropic-key",
        "model": "claude-3-sonnet"
      }
    },
    "caching": {
      "redis": {
        "url": "your-redis-url",
        "ttl": 3600
      }
    }
  },
  "deployment": {
    "platform": "vercel",
    "environment": "production",
    "region": "us-east-1",
    "scaling": {
      "min_instances": 1,
      "max_instances": 10
    }
  }
}
