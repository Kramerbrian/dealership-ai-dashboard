{
  "name": "dealershipai-dashboard",
  "version": "1.0.0",
  "description": "AI-powered dealership analytics dashboard with algorithmic visibility insights",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=__tests__",
    "test:e2e": "playwright test",
    "analyze": "ANALYZE=true next build",
    "cleanup:todos": "node scripts/cleanup-todos.js",
    "cleanup:todos:detailed": "node scripts/cleanup-todos.js --detailed",
    "cleanup:todos:action-plan": "node scripts/cleanup-todos.js --action-plan",
    "cleanup:todos:report": "node scripts/cleanup-todos.js --markdown",
    "setup": "./quickstart.sh",
    "deploy": "./deploy-production.sh"
  },
  "keywords": [
    "dealership",
    "ai",
    "dashboard",
    "analytics",
    "automotive",
    "visibility",
    "seo",
    "marketing"
  ],
  "author": "DealershipAI Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.9.0",
    "@aws-sdk/client-bedrock-runtime": "^3.901.0",
    "@boxyhq/saml-jackson": "^0.4.2",
    "@clerk/nextjs": "^6.33.3",
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@ory/client": "^1.22.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.0",
    "@tanstack/react-query": "^5.90.2",
    "@trpc/client": "^11.6.0",
    "@trpc/next": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "express": "^5.1.0",
    "framer-motion": "^12.23.22",
    "googleapis": "^161.0.0",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "next-auth": "^4.24.11",
    "openai": "^4.20.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.2.1",
    "stripe": "^19.1.0",
    "superjson": "^2.2.2",
    "svix": "^1.76.1",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.50.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.0",
    "typescript": "^5.2.0",
    "wait-on": "^7.2.0",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dealershipai/dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/dealershipai/dashboard/issues"
  },
  "homepage": "https://dealershipai.com",
  "files": [
    "app/**/*",
    "src/**/*",
    "public/**/*",
    "*.config.*",
    "README.md"
  ]
}
